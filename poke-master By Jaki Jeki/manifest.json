{
    "manifest_version": 3,
    "name": "Poke Master 3000",
    "version": "1.0",
    "description": "Auto poke semua teman di Pesnuk BY Jaki Jeki",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "hand.png"
